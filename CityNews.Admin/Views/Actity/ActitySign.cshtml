@model CityNews.Model.Common.ResultList<CityNews.Model.CityNews_ActitySign>
@{
    ViewBag.Title = "报名列表";
}

<h2 class="text-primary">报名列表</h2>
<div style="width: 50%; margin: 20px 0;float: right"><a style="float: right" href='@Url.Action("Export",new {id= Html.ViewContext.RouteData.Values["id"] })' class="btn btn-primary">导出</a></div>
@Html.AntiForgeryToken()
<div id="gettable">
    <table class="table table-hover">
        <thead>
            <tr>
                <th>姓名</th>
                <th>电话</th>
                <th>报名时间</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.Data)
            {
                <tr>
                    <td>
                        @item.Name
                    </td>
                    <td>
                        @item.Phone
                    </td>
                    <td>
                        @item.AddTime.ToString("yyyy-MM-dd HH:mm")
                </td>
            </tr>
            }
        </tbody>
    </table>
</div>




